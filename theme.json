{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "1140px",
			"wideSize": "1240px"
		},
		"spacing": {
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"defaultFontSizes": true,
			"fontFamilies": [
				{
					"fontFamily": "Cormorant Garamond",
					"name": "Cormorant Garamond",
					"slug": "cormorant-garamond",
					"fontFace": [
						{
							"src": [
								"file:./src/fonts/cormorant-garamond/CormorantGaramond.woff2"
							],
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontFamily": "Cormorant Garamond"
						}
					]
				},
				{
					"fontFamily": "Jost Regular",
					"name": "Jost Regular",
					"slug": "jost-regular",
					"fontFace": [
						{
							"src": [	
								"file:./src/fonts/Jost-Regular.woff2"
							],
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontFamily": "Jost Regular"
						}
					]
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|jost-regular",
			"fontWeight": "400",
			"letterSpacing": "0.65px",
			"lineHeight": "1.4"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"patterns": [
		"version-original/header"
	],
	"version": 3
}
